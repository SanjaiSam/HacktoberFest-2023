#include <bits/stdc++.h>
using namespace std;

// Recursive solution
int solve(vector<int> &num, int x) {
    if (x == 0) {
        return 0;
    }
    if (x < 0) {
        return INT_MAX;
    }
    int mini = INT_MAX;
    for (int i = 0; i < num.size(); i++) {
        int ans = solve(num, x - num[i]);
        if (ans != INT_MAX) {
            mini = min(mini, 1 + ans);
        }
    }
    return mini;
}

int minimumElementsRecursive(vector<int> &num, int x) {
    int ans = solve(num, x);
    return (ans == INT_MAX) ? -1 : ans;
}

// Recursive + Memoization solution
int solvemem(vector<int> &num, int x, vector<int> &dp) {
    if (x == 0) {
        return 0;
    }
    if (x < 0) {
        return INT_MAX;
    }
    if (dp[x] != -1) {
        return dp[x];
    }
    int mini = INT_MAX;
    for (int i = 0; i < num.size(); i++) {
        int ans = solvemem(num, x - num[i], dp);
        if (ans != INT_MAX) {
            mini = min(mini, 1 + ans);
        }
    }
    dp[x] = mini;
    return dp[x];
}

int minimumElementsMemoization(vector<int> &num, int x) {
    vector<int> dp(x + 1, -1);
    int ans = solvemem(num, x, dp);
    return (ans == INT_MAX) ? -1 : ans;
}

// Tabulation solution
int solvetab(vector<int> &num, int x) {
    vector<int> dp(x + 1, INT_MAX);
    dp[0] = 0;  // Base case: 0 coins needed to make 0 amount

    for (int i = 1; i <= x; i++) {
        for (int j = 0; j < num.size(); j++) {
            if (i - num[j] >= 0 && dp[i - num[j]] != INT_MAX) {
                dp[i] = min(dp[i], 1 + dp[i - num[j]]);
            }
        }
    }
    return (dp[x] == INT_MAX) ? -1 : dp[x];
}

int minimumElementsTabulation(vector<int> &num, int x) {
    return solvetab(num, x);
}

int main() {
    vector<int> num = {1, 2, 3}; // Example input vector
    int x = 5; // Example value of x

    // Uncomment one of the following lines to test a specific method

    // Using recursive approach
    // int result = minimumElementsRecursive(num, x);

    // Using memoization approach
    // int result = minimumElementsMemoization(num, x);

    // Using tabulation approach
    int result = minimumElementsTabulation(num, x);

    cout << "Minimum number of elements required to form " << x << " is: " << result << endl;
    return 0;
}
